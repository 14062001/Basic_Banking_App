<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.basic_banking_app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Basic_Banking_App" >
        <activity android:name="com.example.basic_banking_app.finalactivity" />
        <activity android:name="com.example.basic_banking_app.Viewall" />
        <activity android:name="com.example.basic_banking_app.transfermoney" />
        <activity
            android:name="com.example.basic_banking_app.select"
            android:label="@string/title_activity_select" />
        <activity android:name="com.example.basic_banking_app.Home" />
        <activity android:name="com.example.basic_banking_app.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>